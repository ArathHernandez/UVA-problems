#include <iostream>
#include <algorithm>
using namespace std;

int main()
{
    int tests; cin >> tests;
    
    for(int i = 0; i < tests; i++){
        int salarys[3];
        
        cin >> salarys[0] >> salarys[1] >> salarys[2];
        
        sort(salarys, salarys+3);
        
        cout << "Case " << i+1 << ": " << salarys[1] << endl;
    }

    return 0;
}
