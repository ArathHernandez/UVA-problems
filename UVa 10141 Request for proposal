#include <iostream>
#include <unordered_map>
#include <string>

using namespace std;

int main()
{
    int n, p;
    int a = 1;
    
    while(cin >> n >> p){
        if(n==0&&p==0) break;
        unordered_map<string,int>req;
        string bestM;
        float lowestP = 0;
        int bestR = 0;
        for(int i = 0; i < n; i++){
            string aux; cin.getline(aux);
            req[aux] = 1;
        }
        
        for(int j = 0; j < p; j++){
            cin.ignore();
            string mark; getline(cin,mark);
            float price; cin >> price;
            int tempR; cin >> tempR;
            
            cout << mark << " " << price << " " << tempR << endl;
            
            int tempA = 0;
            
            for(int i = 0; i < tempR; i++){
                cin.ignore();
                string aux; getline(cin,aux);
                if(req[aux] == 1) tempA++;
            }
            
            if(tempA > bestR){
                bestM = mark;
                lowestP = price;
                bestR = tempA;
            }
            
            if(tempA == bestR){
                if(price < lowestP){
                    bestM = mark;
                    lowestP = price;
                }
            }
        }
        
        cout << "RFP #" << a << endl;
        cout << bestM << endl;
        
        a++;
    }

    return 0;
}
